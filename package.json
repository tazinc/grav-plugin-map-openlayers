{
  "name": "map-openlayers",
  "version": "0.0.1",
  "description": "The **Map OpenLayers** Plugin is for [Grav CMS](http://github.com/getgrav/grav). Embed a map with markers using fontawesome icons or letters/numbers. Uses open source [OpenLayers js library](https://openlayers.org) with data from [OpenStreetMap](https://www.openstreetmap.org). Enhanced map styling can be obtained from  [Thunderforest](https://www.thunderforest.com) or [MapBox](https://www.mapbox.com), but requires an api key (royalty free options are available).",
  "private": true,
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@rollup-extras/plugin-clean": "^1.3.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.1",
    "cssnano": "^6.1.2",
    "minimist": "^1.2.8",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "rollup": "^4.16.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "rollup -c",
    "build:wp": "webpack --config webpack.config.js"
  },
