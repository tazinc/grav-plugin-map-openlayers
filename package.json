{
  "name": "map-openlayers",
  "version": "0.0.1",
  "description": "The **Map OpenLayers** Plugin is for [Grav CMS](http://github.com/getgrav/grav). Embed a map with markers using fontawesome icons or letters/numbers. Uses open source [OpenLayers js library](https://openlayers.org) with data from [OpenStreetMap](https://www.openstreetmap.org). Enhanced map styling can be obtained from  [Thunderforest](https://www.thunderforest.com) or [MapBox](https://www.mapbox.com), but requires an api key (royalty free options are available).",
  "private": true,
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@rollup-extras/plugin-clean": "^1.3.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "minimist": "^1.2.8",
    "npm-run-all2": "^7.0.2",
    "npm-watch": "^0.13.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "rollup": "^4.37.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "npm-watch"
  },
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.6.5"
  },
  "dependencies": {
    "ol": "10.4.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "js"
      ],
      "extensions": "ts",
      "quiet": "false"
    }
  }
}
